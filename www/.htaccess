RewriteEngine On
RewriteBase /
#RewriteMap lowercase int:tolower
#AllowEncodedSlashes On

# Deny any Urls for /application
RedirectMatch 404 ^/application(/?|/.*)$

# Don't serve anything ending in .ini
<FilesMatch "[^\.]+\.ini">
	Order allow,deny
	Deny from all
	Satisfy All
</FilesMatch>

